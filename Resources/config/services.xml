<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="phaidon_piwik_module.container.link_container.class">Phaidon\PiwikModule\Container\LinkContainer</parameter>
        <parameter key="phaidon_piwik_module.helper.piwik_data_helper.class">Phaidon\PiwikModule\Helper\PiwikDataHelper</parameter>
        <parameter key="phaidon_piwik_module.helper.user_output_helper.class">Phaidon\PiwikModule\Helper\UserOutputHelper</parameter>
        <parameter key="phaidon_piwik_module.helper.dashboard_helper.class">Phaidon\PiwikModule\Helper\DashboardHelper</parameter>
        <parameter key="phaidon_piwik_module.tracker_code_injector_listener.class">Phaidon\PiwikModule\Listener\TrackerCodeInjectorListener</parameter>
    </parameters>
    <services>
        <service id="phaidon_piwik_module.container.link_container" class="%phaidon_piwik_module.container.link_container.class%">
            <argument type="service" id="translator.default" />
            <argument type="service" id="jms_i18n_routing.router" />
            <argument type="service" id="zikula_permissions_module.api.permission" />
            <argument type="service" id="phaidon_piwik_module.helper.piwik_data_helper" />
            <tag name="zikula.link_container" />
        </service>

        <service id="phaidon_piwik_module.helper.piwik_data_helper" class="%phaidon_piwik_module.helper.piwik_data_helper.class%" lazy="true">
            <argument type="service" id="translator.default" />
            <argument type="service" id="zikula_extensions_module.api.variable" />
            <argument type="service" id="session" />
            <argument type="service" id="request_stack" />
        </service>

        <service id="phaidon_piwik_module.helper.user_output_helper" class="%phaidon_piwik_module.helper.user_output_helper.class%" lazy="true">
            <argument type="service" id="zikula_extensions_module.api.variable" />
            <argument type="service" id="phaidon_piwik_module.helper.piwik_data_helper" />
            <argument type="service" id="twig" />
        </service>

        <service id="phaidon_piwik_module.helper.dashboard_helper" class="%phaidon_piwik_module.helper.dashboard_helper.class%" lazy="true">
            <argument type="service" id="phaidon_piwik_module.helper.piwik_data_helper" />
            <argument type="service" id="twig" />
        </service>

        <service id="phaidon_piwik_module.tracker_code_injector_listener" class="%phaidon_piwik_module.tracker_code_injector_listener.class%">
            <tag name="kernel.event_subscriber" />
            <argument type="service" id="kernel" />
            <argument type="service" id="phaidon_piwik_module.helper.user_output_helper" />
        </service>
    </services>
</container>
